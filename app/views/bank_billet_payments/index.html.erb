<p id="notice"><%= notice %></p>

<h1>Listing Bank Billet Payments</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @bank_billet_payments.each do |bank_billet_payment| %>
      <tr>
        <td><%= link_to 'Show', bank_billet_payment %></td>
        <td><%= link_to 'Edit', edit_bank_billet_payment_path(bank_billet_payment) %></td>
        <td><%= link_to 'Destroy', bank_billet_payment, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

